  <script type="text/javascript">

  var password = document.getElementById("password")
    , passwords = document.getElementById("confirmPassword");

  function validatePassword(){
    if(password.value != passwords.value) {
      passwords.setCustomValidity("Passwords Don't Match");
    } else {
      passwords.setCustomValidity('');
    }
  }

  password.onchange = validatePassword;
  passwords.onkeyup = validatePassword;

  </script>

