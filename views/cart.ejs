              <% if(user){ %>
            <!--Welcome...!!--><li><a href=""><span class="fa fa-user"></span>
            <% if (user.twitter.username != null) { %>
            <%= user.twitter.username %>
            <% } else if (user.google.email != null) { %>
            <%= user.google.email %>
            <% } else if (user.facebook.name  != null)  { %>
